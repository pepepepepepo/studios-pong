{
  "name": "studios-pong",
  "displayName": "Studios Pong",
  "description": "Your AI Persona Team in VS Code - 74 AI Personas with Local Memory & ResonanceEngine",
  "version": "0.0.1",
  "publisher": "studios-pong",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "AI"
  ],
  "keywords": [
    "ai",
    "personas",
    "chat",
    "assistant",
    "local memory",
    "resonance engine"
  ],
  "activationEvents": [
    "onCommand:studios-pong.openChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "studios-pong.openChat",
        "title": "Studios Pong: Open AI Persona Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
